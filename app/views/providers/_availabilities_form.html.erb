<%= form_with model: [@provider, @availability], local: true, data: { turbo_frame: "new_availability" } do |form| %>
  <%= form.collection_select :service_id, @provider.services, :id, :name, prompt: 'Select a Service' %>
  <%= form.datetime_field :start_time %>
  <%= form.datetime_field :end_time %>
  <%= form.submit "Add Availability" %>
<% end %>
